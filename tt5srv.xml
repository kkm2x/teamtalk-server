        </user>
    </users>
    <permanent-channels>
        <channel>
            <channel-id>1</channel-id>
            <password></password>
            <topic>Main Channel</topic>
            <diskquota>0</diskquota>
            <op-password></op-password>
            <max-users>1000</max-users>
            <channel-type>1</channel-type>
            <userdata>0</userdata>
            <audio-codec>
                <codec-type>1</codec-type>
                <bandmode>1</bandmode>
                <quality>4</quality>
