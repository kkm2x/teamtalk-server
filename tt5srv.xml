<?xml version="1.0" encoding="UTF-8" ?>
<teamtalk version="5.21">
    <general>
        <server-name>Manus Permanent Server</server-name>
        <motd>Welcome to your permanent TeamTalk server!</motd>
        <max-users>1000</max-users>
        <auto-save>true</auto-save>
        <bind-ip />
        <tcpport>8000</tcpport>
        <udpport>8000</udpport>
        <login-attempts>0</login-attempts>
        <max-logins-per-ipaddr>0</max-logins-per-ipaddr>
        <user-timeout>60</user-timeout>
        <login-delay-msec>2000</login-delay-msec>
